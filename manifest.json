    "calendar_ev_insert": {
      "title": "Insert a Calendar Event",
      "description": "Creates a Google Calendar Event With Structured Data",
      "trigger": "invoke",
      "docref" : "https://developers.google.com/google-apps/calendar/v3/reference/events/insert",
      "config": {
        "properties": {},
        "definitions": {}
