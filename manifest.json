      "exports": {
        "properties": {
          "status": {
            "type": "string",
            "title": "Status"
          },
          "id": {
            "type": "string",
            "title": "ID"
          },
          "created": {
            "type": "string",
            "title": "Created Time"
          },
          "summary": {
            "type": "string",
            "title": "Summary"
          },
          "description": {
            "type": "string",
            "title": "Description"
          },
          "location": {
            "type": "string",
            "title": "Location"
          },
          "start_date": {
            "type": "string",
            "title": "Start Date",
            "description" : "A combined date-time value. A time zone offset is required unless a time zone is explicitly specified in Start TimeZone."
          },
          "start_datetime": {
            "type": "string",
            "title": "Start DateTime"
          },
          "start_timezone": {
            "type": "string",
            "title": "Start TimeZone"
