          "start_timezone": {
            "type": "string",
            "title": "Start TimeZone"
          },
          "end_date": {
            "type": "string",
            "title": "End Date"
          },
          "end_datetime": {
            "type": "string",
            "title": "End DateTime"
          },
          "end_timezone": {
            "type": "string",
            "title": "End TimeZone"
          }
        },
        "required": [
          "text",
          "attendees_emails"
        ],
        "disposition": [
          "text",
          "attendees_emails",
          "reminder_method",
          "reminder_minutes",
          "location",
          "start_date",
          "start_datetime",
          "start_timezone",
          "end_date",
          "end_datetime",
          "end_timezone"
        ]
      }
